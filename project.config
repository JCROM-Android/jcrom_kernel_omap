[receive]
	requireSignedOffBy = true
[submit]
	action = fast forward only
[project]
	state = active
[access]
	inheritFrom = Kernel-Projects
[access "refs/heads/*"]
	owner = group kernel-owners
[access "refs/heads/android-omap-panda-3.0"]
	label-Code-Review = -2..+2 group JBQ
	label-Verified = -1..+1 group JBQ
	submit = group JBQ
[access "^refs/heads/glass-omap-.*"]
	create = group glass-kernel-pushers
	forgeCommitter = group glass-kernel-pushers
	push = +force group glass-kernel-pushers
	pushMerge = group glass-kernel-pushers
